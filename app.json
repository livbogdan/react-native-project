{
  "expo": {
    "name": " Guide App",
    "slug": "GuideAppSlug",
    "version": "0.0.01 beta",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "ios": {
      "jsEngine": "jsc"},
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#6C4AB6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.livbogdan.Guide.App",
      "supportsTablet": true
    },
    "android": {
      "package": "com.livbogdan.Guide.App",
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    
  }
}
